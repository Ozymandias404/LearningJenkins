pipeline {
agent none
    stages {
        stage('Python') {
            agent {
                docker { image 'python:3.11.0rc1-bullseye' }
            }
            steps {
                git branch: 'main', url: 'https://github.com/Ozymandias404/LearningJenkinsPython'
                sh 'python --version'
                sh 'ls -a'
                sh 'pwd'
                sh 'cd Scripts'
                sh 'ls -a'
                sh 'pwd'
            }
        }
    }
}

